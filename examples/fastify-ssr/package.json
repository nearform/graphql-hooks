{
  "name": "fastify-ssr",
  "version": "3.9.2",
  "private": true,
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=12.0.0"
  },
  "exports": "./index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "webpack",
    "build:server": "babel src -d lib --copy-files",
    "prebuild": "rm -rf build lib",
    "build": "npm run build:server && npm run build:client",
    "start": "node ./lib/index.js",
    "watch:client": "webpack --watch",
    "watch:server": "nodemon --watch src --ignore src/client --exec 'npm run build:server && npm run start | pino-pretty'",
    "prewatch": "npm run prebuild",
    "watch": "npm run build:client && npm run watch:server & npm run watch:client"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "fastify": "^3.0.0",
    "fastify-gql": "^5.0.0",
    "fastify-static": "^4.5.0",
    "graphql-hooks": "^5.11.2",
    "graphql-hooks-memcache": "^2.3.1",
    "graphql-hooks-ssr": "^2.2.3",
    "isomorphic-unfetch": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^6.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "nodemon": "^2.0.0",
    "pino-pretty": "^7.2.0",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": "> 0.25%, not dead",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          },
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "dynamic-import-node"
    ]
  }
}
