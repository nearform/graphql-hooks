{
  "name": "fastify-ssr",
  "version": "5.0.1",
  "private": true,
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=12.0.0"
  },
  "exports": "./index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "webpack",
    "build:server": "babel src -d lib --copy-files",
    "prebuild": "rm -rf build lib",
    "build": "npm run build:server && npm run build:client",
    "start": "node ./lib/index.js",
    "watch:client": "webpack --watch",
    "watch:server": "nodemon --watch src --ignore src/client --exec 'npm run build:server && npm run start | pino-pretty'",
    "prewatch": "npm run prebuild",
    "watch": "npm run build:client && npm run watch:server & npm run watch:client"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "fastify": "^4.2.0",
    "graphql-hooks": "^7.0.0",
    "graphql-hooks-memcache": "^3.2.0",
    "graphql-hooks-ssr": "^3.0.1",
    "isomorphic-unfetch": "^4.0.2",
    "mercurius": "^14.0.0",
    "prop-types": "^15.7.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^9.1.0",
    "nodemon": "^3.0.1",
    "pino-pretty": "^11.1.0",
    "webpack": "^5.64.4",
    "webpack-cli": "^5.0.1",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": "> 0.25%, not dead",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          },
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "dynamic-import-node"
    ]
  }
}
