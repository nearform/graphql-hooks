{
  "compilerOptions": {
    "lib": ["es2021"],
    "module": "esnext",
    "target": "es2021",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "noFallthroughCasesInSwitch": true
  }
}
